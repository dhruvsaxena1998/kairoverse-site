---
const { classlist } = Astro.props;
const menu_items = [
  { slug: "portfolio", label: "Portfolio" },
  { slug: "about", label: "About" },
  { slug: "/blog/journey", label: "Blog" },
  { slug: "contact", label: "Contact US" },
];
---

<ul class="flex gap-4">
  {
    menu_items.map((item) => (
      <li
        class:list={["capitalize font-medium", classlist]}
      >
        <a href={item.slug}>{item.label}</a>
      </li>
    ))
  }
</ul>
